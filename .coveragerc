# .coveragerc to control coverage.py
[run]
branch = True

omit =
    */__init__.py
    */*_test.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self\.debug
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:

ignore_errors = True

